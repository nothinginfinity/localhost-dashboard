{
  "services": [
    {
      "name": "Lawyers & Dragons Frontend",
      "port": 3000,
      "path": "~/ztgi/lawyers_and_dragons/frontend",
      "type": "frontend",
      "startCmd": "npm run dev",
      "github": "https://github.com/nothinginfinity/lawyers-and-dragons",
      "endpoints": ["/", "/browse", "/game", "/mock-trial", "/mediation"]
    },
    {
      "name": "Lawyers & Dragons Dashboard",
      "port": 5173,
      "path": "~/ztgi/lawyers_and_dragons/dashboard",
      "type": "frontend",
      "startCmd": "npm run dev",
      "github": "https://github.com/nothinginfinity/lawyers-and-dragons",
      "endpoints": ["/", "/leads", "/analytics", "/settings"]
    },
    {
      "name": "Lawyers & Dragons Lawyer Portal",
      "port": 3007,
      "path": "~/ztgi/lawyers_and_dragons/lawyer-portal",
      "type": "frontend",
      "startCmd": "npm run dev",
      "github": "https://github.com/nothinginfinity/lawyers-and-dragons",
      "endpoints": ["/", "/cases", "/clients"]
    },
    {
      "name": "Lawyers & Dragons API",
      "port": 8000,
      "path": "~/ztgi/lawyers_and_dragons/backend",
      "type": "backend",
      "startCmd": "uvicorn main:app --host 0.0.0.0 --port 8000",
      "github": "https://github.com/nothinginfinity/lawyers-and-dragons",
      "endpoints": ["/docs", "/api/game/sessions", "/api/clients"]
    },
    {
      "name": "Email-for-AI Dashboard",
      "port": 8001,
      "path": "~/ztgi/email-for-ai/dashboard",
      "type": "frontend",
      "startCmd": "npm run dev",
      "github": "https://github.com/nothinginfinity/email-for-ai",
      "endpoints": ["/", "/projects", "/agents"]
    },
    {
      "name": "Email-for-AI Backend",
      "port": 8010,
      "path": "~/ztgi/email-for-ai",
      "type": "backend",
      "startCmd": "python3 main.py",
      "github": "https://github.com/nothinginfinity/email-for-ai",
      "endpoints": ["/docs"]
    },
    {
      "name": "PHI Proxy",
      "port": 8080,
      "path": "~/ztgi/phi_proxy",
      "type": "api",
      "startCmd": "python3 main.py",
      "github": "https://github.com/nothinginfinity/prax-proxy",
      "endpoints": ["/"]
    },
    {
      "name": "Copy Not Paste - Browser",
      "port": 7891,
      "path": "~/ztgi/copy_not_paste",
      "type": "frontend",
      "startCmd": "python3 cnp_browser/app.py",
      "github": "https://github.com/nothinginfinity/copy-not-paste",
      "endpoints": ["/", "/timeline", "/ask"]
    },
    {
      "name": "Copy Not Paste - API",
      "port": 7890,
      "path": "~/ztgi/copy_not_paste",
      "type": "api",
      "startCmd": "python3 cnp_api.py",
      "github": "https://github.com/nothinginfinity/copy-not-paste",
      "endpoints": ["/", "/health", "/history", "/search", "/resolve"]
    },
    {
      "name": "ZTGI UI Library",
      "port": 7892,
      "path": "~/ztgi/ztgi-ui",
      "type": "api",
      "startCmd": "node server.js",
      "github": "https://github.com/nothinginfinity/ztgi-ui",
      "endpoints": ["/", "/ztgi-ui.js", "/configs", "/health"]
    },
    {
      "name": "Localhost Dashboard",
      "port": 9999,
      "path": "~/ztgi/localhost-dashboard",
      "type": "frontend",
      "startCmd": "node server.js",
      "github": "",
      "endpoints": ["/", "/api/services", "/api/status"]
    }
  ]
}
